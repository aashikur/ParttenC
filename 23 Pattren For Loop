#include <stdio.h>

int main()
{

    printf("\n\n"); // Break
    //========================================
    // Question 23

    for(int a = 5 ; a>=1; a--)
    {
        for(int b = 1; b<=a ; b++) // 1 2 3 4 5
        {
            if(a%2 ==1 )
                printf("%d ",b);
        }
        for(int c = a; c>=1; c--) // 4 3 2 1
        {
            if(a%2==0)
                printf("%d ", c);
        }
        printf("\n");
    }




    return 0;
}
